{
  "name": "vue-treeselectjs",
  "version": "0.9.1",
  "description": "Treeselect Vue Component",
  "main": "./dist/vue-treeselectjs.umd.js",
  "module": "./dist/vue-treeselectjs.mjs",
  "types": "./dist/Treeselect.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/Treeselect.d.ts",
        "default": "./dist/vue-treeselectjs.mjs"
      },
      "require": {
        "types": "./dist/Treeselect.d.ts",
        "default": "./dist/vue-treeselectjs.umd.js"
      }
    },
    "./dist/vue-treeselectjs.css": {
      "import": "./dist/vue-treeselectjs.css",
      "require": "./dist/vue-treeselectjs.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "biome lint .",
    "check": "biome check --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipson88/vue-treeselectjs.git"
  },
  "keywords": [
    "vue",
    "treeselectjs",
    "treeselect",
    "select",
    "groupselect",
    "js",
    "ts",
    "typescript",
    "multi-select",
    "multiple-select"
  ],
  "author": "Dzmitry Zhuraukou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dipson88/vue-treeselectjs/issues"
  },
  "homepage": "https://github.com/dipson88/vue-treeselectjs#readme",
  "dependencies": {
    "treeselectjs": "^0.14.1"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@biomejs/biome": "^2.4.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "typescript": "^5.8.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.29",
    "vue-tsc": "^3.2.5"
  },
  "peerDependencies": {
    "vue": "^3.x"
  },
  "overrides": {
    "minimatch": ">=10.2.4"
  },
  "files": [
    "dist"
  ]
}
